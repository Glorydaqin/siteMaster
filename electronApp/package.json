{
  "name": "electron-tabs",
  "version": "0.15.0",
  "description": "Simple tabs for Electron applications",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/brrd/electron-tabs"
  },
  "build": {
    "appId": "com.vipshare.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "scripts": {
    "start": "electron main",
    "watch": "nodemon --watch main/* --exec \\\"electron ./main\\\"",
    "dist": "electron-builder --win --x64"
  },
  "keywords": [
    "electron",
    "tab",
    "tabs",
    "webview"
  ],
  "author": "brrd",
  "license": "MIT",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^8.0.0",
    "electron-builder": "^22.9.1",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "dragula": "^3.7.3",
    "electron-tabs": "^0.15.0"
  }
}
