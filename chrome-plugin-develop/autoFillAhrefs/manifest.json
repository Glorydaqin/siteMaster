{
  "name": "autoFillAhrefs",
  "version": "1",
  "description": "Auto Fill Ahrefs",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png"
  },
  "background": {
    "scripts": [
      "js/jquery2.1.4.min.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "<all_urls>",
    "webRequest",
    "management"
  ]
}
